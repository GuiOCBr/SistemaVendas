@model List<SistemaVenda.Entities.Category>;

<h2>Categoria</h2>
<hr/>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Código</th>
            <th>Descrição</th>
        </tr>
    </thead>
    <tbody>
            @foreach(var item in Model)
            {
                <tr>
                    <td>@item.Code</td>
                    <td>@item.Description</td>
                </tr>
            }
    </tbody>
</table>